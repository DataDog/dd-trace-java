      "type": "array",
      "items": {
        "title": "Parameter 0.1.0",
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      }
    },
    "highlight": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "has_server_side_match": {
      "type": "boolean",
      "description": "This field is not currently used, it was added for backward compatibility"
    }
  }
}
