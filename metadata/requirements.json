{
  "version": 1,
  "deny": [
    {
      "id": "unsupported_jvm",
      "description": "Skip older JVMs",
      "os": null,
      "cmds": [
        "**/java-1.5*/**/java",
        "**/java-1.6*/**/java",
        "**/java-6*/**/java",
        "**/java-7/**/java"
      ],
      "args": [],
      "envars": null
    },
    {
      "id": "java8_version",
      "description": "Skip java -version command",
      "os": null,
      "cmds": [
        "**/java"
      ],
      "args": [
        {
          "args": [
            "-version"
          ],
          "position": 1
        }
      ],
      "envars": null
    },
    {
      "id": "java_version",
